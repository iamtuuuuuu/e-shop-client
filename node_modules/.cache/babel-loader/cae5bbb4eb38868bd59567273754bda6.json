 default: 0 },\n        step: { type: Number, default: 1 }\n    },\n    emits: {\n        'update:modelValue': null\n    },\n    mounted() {\n        this.triggerLabelActiveClass()\n    },\n    updated() {\n        this.triggerLabelActiveClass()\n    },\n    methods: {\n        triggerLabelActiveClass() {\n            const input = document.getElementById(this.inputId)\n            input.value.length !== 0 ? input.classList.add('active') : input.classList.remove('active')\n        }\n    }\n}\n</script>\n\n<style scoped lang=\"scss\">\n// .mb-5 { margin-bottom: 2.25rem !important }\n\n.form-group {\n    position: relative;\n    background-color: var(--white);\n\n    label {\n        position: absolute;\n        top: 26%; left: 1.06rem;\n        z-index: 1;\n        cursor: text;\n        transition: all .1s ease-out;\n        color: var(--gray);\n    }\n    input.active ~ label,\n    input:focus ~ label {\n        transform: translateY(-1.75em);\n        background-color: inherit;\n        padding: 0 .25rem;\n        font-size: .8em;\n        font-weight: 700;\n    }\n    input:focus ~ label { color: var(--primary) }\n    input.is-invalid ~ label { color: var(--danger) }\n\n    input {\n        height: calc(1.5em + 1.5rem + 2px);\n        padding: .75rem 1rem;\n\n        &:focus {\n            border-color: var(--primary);\n            border-width: 2px;\n        }\n        &.is-invalid { border-color: var(--danger) }\n    }\n\n    span.invalid-feedback {\n        position: absolute;\n        top: 100%; left: 0;\n    }\n}\n</style>\n"],"sourceRoot":""}]}